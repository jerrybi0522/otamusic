<%= @user.username %><br>
<%= @user.email %><br>
<% if logged_in? %>
	<% if current_user == @user || current_user.admin %>
		<%= link_to "Edit User", edit_user_path(@user) %>
		<%= link_to "New Song", new_song_path %>
	<% end %>
<% end %>
<br><br>

Uploaded Songs
<br><br>
<% @songs.reverse.each do |song| %>
	<%= link_to "#{song.name} by #{song.artist}", song_path(song) %><br>
<% end %>
<br><br>

Favorites
<br><br>
<% @fav_songs.reverse.each do |fav| %>
	<%= link_to "#{fav.song.name} by #{fav.song.artist}", song_path(fav) %><br>
<% end %>