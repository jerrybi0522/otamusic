<%= @user.username %>
<%= @user.email %>
<% if logged_in? %>
	<% if current_user == @user || current_user.admin %>
		<%= link_to "Edit User", edit_user_path(@user) %>
		<%= link_to "New Song", new_song_path %>
	<% end %>
<% end %>

<% @songs.reverse.each do |song| %>
	<%= link_to "#{song.name} by #{song.artist}", song_path(song) %>
<% end %>
<br>

<% @fav_songs.reverse.each do |fav| %>
	<%= link_to "#{fav.song.name} by #{fav.song.artist}", song_path(fav) %>
<% end %>