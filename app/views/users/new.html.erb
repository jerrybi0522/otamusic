<%= form_for @user do |f| %>
	<%= f.text_field :username, placeholder: "Username" %><br><br>

	<%=f.password_field :password, placeholder: "Password" %><br><br>

	<%=f.password_field :password_confirmation, placeholder: "Re-enter Password" %><br><br>

	<%=f.email_field :email, placeholder: "Email" %><br><br>

	<%=f.submit "Create User" %><br>
<% end %>