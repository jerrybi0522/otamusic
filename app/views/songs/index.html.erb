All Songs
<br><br>

<div class="wrapper">
	<% @categories.each do |category| %>
		<div class="song_column <%=category.mood%>">
			<%= category.mood %><br><br>
			<% category.songs.each do |song| %>
				<%= link_to "#{song.name} by #{song.artist}", song_path(song), class: "song_link" %><br>
			<% end %><br>
		</div>
	<% end %>
</div>