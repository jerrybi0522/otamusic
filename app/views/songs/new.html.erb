<%= form_for @song do |f| %>
	<%= f.text_field :name, placeholder: "Song Name", :maxlength => 150 %><br><br>
	<%= f.text_field :artist, placeholder: "Song Artist", :maxlength => 150 %><br><br>
	<%= f.text_field :youtube_url, placeholder: "Youtube URL", :maxlength => 150 %><br><br>
	<%= f.text_area :description, :cols => 50, :rows => 10 %><br><br>
<% end %>